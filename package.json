{
  "name": "vitepress-templ-mono",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm build:package",
    "build:package": "pnpm --filter='vitepress-templ-preview' run build",
    "demo": "pnpm run build && pnpm --filter='vitepress-templ-preview-demo' run docs:dev",
    "format": "prettier --write .",
    "prepare": "husky",
    "prepublish": "pnpm build",
    "release": "pnpm publish --filter='vitepress-templ-preview' --access public"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "husky": "^9.0.11",
    "prettier": "^3.3.2",
    "prettier-plugin-packagejson": "^2.5.0"
  },
  "packageManager": "pnpm@9.4.0",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=10.0.0"
  }
}
